{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "docker compose -f docker-compose.dev.yml up --build",
    "dev:local": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "dev:down": "docker compose  -f docker-compose.dev.yml down && docker compose -f docker-compose.dev.yml down -v",
    "down": "docker compose -f docker-compose.yml down && docker compose down -v",
    "db-dev:migrate-with-seed": "TS_NODE_PROJECT=./tsconfig.json bun x tsx -r tsconfig-paths/register ./src/db/migrate-seed-db.ts",
    "db-prod:migrate-with-seed": " node  ./dist/db/migrate-seed-db.js",
    "build": "docker compose -f docker-compose.yml up --build",
    "build:prod": "tsc  && tsc-alias -p tsconfig.json",
    "copy:files": "bunx cpx \"migrations/**\" ./dist/migrations",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "check": "npm run lint && npm run typecheck",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:push": "npx drizzle-kit push",
    "db:pull": "npx drizzle-kit pull",
    "db:up": "npx drizzle-kit up",
    "db:studio": "npx drizzle-kit studio",
    "inngest:dev": "npx --ignore-scripts=false inngest-cli@latest dev -u http://app:4000/api/inngest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/google": "^3.0.10",
    "@ai-sdk/openai": "^3.0.12",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@polar-sh/fastify": "^0.5.3",
    "@polar-sh/sdk": "^0.42.2",
    "@sentry/node": "^10.26.0",
    "@socket.io/redis-streams-adapter": "^0.2.3",
    "ai": "^6.0.39",
    "arctic": "^3.7.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "fastify": "^5.7.1",
    "fastify-plugin": "^5.1.0",
    "handlebars": "^4.7.8",
    "inngest": "^3.49.3",
    "ioredis": "^5.9.2",
    "jose": "^6.1.3",
    "opossum": "^9.0.0",
    "pluralize": "^8.0.0",
    "postgres": "^3.4.8",
    "prom-client": "^15.1.3",
    "qs": "^6.14.1",
    "random-word-slugs": "^0.1.7",
    "rate-limiter-flexible": "^9.0.1",
    "socket.io": "^4.8.3",
    "toposort": "^2.0.2",
    "ulid": "^3.0.2",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/pluralize": "^0.0.33",
    "@types/qs": "^6.14.0",
    "@types/toposort": "^2.0.7",
    "drizzle-kit": "^0.31.8",
    "pino-pretty": "^13.1.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
